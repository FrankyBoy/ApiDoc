@using ApiDoc.Models

@{
    Node mdl = ViewBag.Structure.Last();
    mdl.ChangeNote = "";
    ViewBag.Title = "Edit " + mdl.Title;
}

<h2>@ViewBag.Title</h2>

@if (mdl is Branch)
{
    Html.RenderPartial("Chunks/EditBranch", mdl as Branch);
}
else
{
    Html.RenderPartial("Chunks/EditLeaf", mdl as Leaf);
}