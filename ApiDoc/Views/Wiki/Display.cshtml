@using ApiDoc.Models

@{
    Node mdl = ViewBag.Structure.Last();
    ViewBag.Title = "title";

    Html.RenderPartial("Breadcrumb", (IList<Node>)ViewBag.Structure.Nodes);
    Html.RenderPartial("ItemActions", mdl);
}

@if (mdl is Branch)
{
    Html.RenderPartial("Chunks/DisplayBranch", mdl as Branch);
}
else
{
    Html.RenderPartial("Chunks/DisplayLeaf", mdl as Leaf);
}